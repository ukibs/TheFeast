    <event eventtype="3" enumb="0">
      <action>
        <libid>1</libid>
        <id>603</id>
        <kind>7</kind>
        <userelative>0</userelative>
        <isquestion>0</isquestion>
        <useapplyto>-1</useapplyto>
        <exetype>2</exetype>
        <functionname></functionname>
        <codestring></codestring>
        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>//Get the keys
    //Movement keys
    //Falta meter joystick
key_left = keyboard_check(vk_left) || gamepad_button_check(0,gp_padl);
key_right = keyboard_check(vk_right) || gamepad_button_check(0,gp_padr);
key_up = keyboard_check(vk_up) || gamepad_button_check(0,gp_padu);
key_down = keyboard_check(vk_down) || gamepad_button_check(0,gp_padd);
    //Action keys
    //Falta meter teclas
    //Y dejar claro como funcionan
key_action_1_pressed = gamepad_button_check_pressed(0, gp_face1) || keyboard_check_pressed(vk_control);
key_action_2_pressed = gamepad_button_check_pressed(0, gp_face2) || keyboard_check_pressed(vk_space);
key_action_3_pressed = gamepad_button_check_pressed(0, gp_face3) || keyboard_check_pressed(vk_enter);
key_action_4_pressed = gamepad_button_check_pressed(0, gp_face4);

//If we are not locked...
if(status_still == false &amp;&amp; status_dead == false){
    //Move the player
    h_direction = -key_left + key_right;
    v_direction = -key_up + key_down; 
    //Save the directions for control
    if(h_direction != 0) last_hDirection = h_direction;
    if(v_direction != 0) last_vDirection = v_direction; 
    //Acceleration
    if(abs(hsp + h_direction) &lt; movement_speed)
        hsp += h_direction;
    if(abs(vsp + v_direction) &lt; movement_speed)
        vsp += v_direction;
    //Decceleration
    if(h_direction == 0 &amp;&amp; hsp != 0)
        hsp -= sign(hsp);
    if(v_direction == 0 &amp;&amp; vsp != 0)
        vsp -= sign(vsp);
    //(To not) collide control
        //Para los saltables
    if(grounded == true){
        while(place_meeting(x + hsp, y, obj_jumpable_terrain) &amp;&amp; hsp != 0){
            hsp -= sign(hsp);
        }
        while(place_meeting(x, y + vsp, obj_jumpable_terrain) &amp;&amp; vsp != 0){
            vsp -= sign(vsp);
        }
    }
        //Para los no saltables
    while(place_meeting(x + hsp, y, obj_unjumpable_terrain) &amp;&amp; hsp != 0){
        hsp -= sign(hsp);
    }
    while(place_meeting(x, y + vsp + distance_to_floor, obj_unjumpable_terrain) &amp;&amp; vsp != 0){
        vsp -= sign(vsp);
    }
    //Control de horizonte
        //De momento con una altura fijada
    while(place_meeting(x, y + vsp + distance_to_floor, obj_background)){
        vsp += 1;
    }
    //Acci칩n de ataque
    if(key_action_1_pressed){
        attack = instance_create(x, y, obj_attack);
        attack.direction = (image_xscale * -90) + 90;
        attack.image_angle = attack.direction;
    }
    //Acci칩n de salto
    if(key_action_2_pressed &amp;&amp; grounded == true){
        grounded = false;
            //
        falling_speed = -jumpspeed;            
    }
    //Control de gravedad
    if(grounded == false){
        falling_speed += 1;
        if(distance_to_floor - falling_speed &lt; 0){
            zsp = distance_to_floor;
            //zsp = 0;
            distance_to_floor = 0;
            falling_speed = 0;
            grounded = true;
        }
        else{
            distance_to_floor -= falling_speed;
            zsp = falling_speed;
        }
    }
    //Aplicamos movimiento en x,y
    x += hsp;
    y += vsp + zsp;
    //
    zsp = 0;
    //Control para no salir de la pantalla
    x = clamp(x, view_xview + sprite_width/2, 
                view_xview + view_wview - sprite_width/2);
    y = clamp(y, view_yview + sprite_height/2, 
                view_yview + view_hview - sprite_height/2);
}
//Control de muerte
if(health &lt;= 0){
    hspeed = 0;
    status_dead = true;
    //De momento hacemos aqui que resetee si mueres
    if(key_action_3_pressed)
        room_restart();
}

//Ya meteremos cambio de sprite y dem치s
</string>
          </argument>
