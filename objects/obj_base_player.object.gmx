v_direction = 0;
last_hDirection = 0;
last_vDirection = 0;
movement_speed = 10;
//Varaibles de salto
zsp = 0;
jumpspeed = 10;
falling_speed = 0;
distance_to_floor = 0;
grounded = false;
//Status variables
status_still = false;
status_dead = false;
</string>
          </argument>
